{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://satscorecalculator.io/$1",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Robots-Tag",
          "value": "noindex"
        }
      ]
    }
  ]
}