{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://satscorecalculator.io/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "sat-score-calculator.vercel.app"
        }
      ]
    }
  ]
}